{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "allowJs": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "rootDir": "./",
    "strict": true
  },
  "ts-node": {
    "swc": true
  },
  "include": [
    "./**/*"
  ]
}
