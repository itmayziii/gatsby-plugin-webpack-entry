{
  "name": "gatsby-plugin-webpack-entry",
  "version": "1.0.1",
  "description": "Gatsby plugin that lets you add additional entry points to the webpack configuration which will be included in your HTML pages.",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc --project tsconfig.json",
    "watch": "npx tsc --project tsconfig.json --watch",
    "package": "npm run build && npx standard-version",
    "test": "ts-node node_modules/.bin/jasmine --config=jasmine.json",
    "test:coverage": "ts-node node_modules/.bin/nyc --exclude-after-remap=false --exclude=\"**/*.spec.ts\" ts-node node_modules/.bin/jasmine --config=jasmine.json",
    "typecheck": "tsc --project tsconfig.test.json --noEmit",
    "lint": "eslint --ext \".ts, .tsx\" src",
    "lint:fix": "eslint --fix --ext \".ts, .tsx\" src",
    "commit": "npx git-cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itmayziii/gatsby-plugin-webpack-entry.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "webpack",
    "entry",
    "point"
  ],
  "author": "Tommy May III <tommymay37@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jasmine": "~4.3.5",
    "@types/node": "~20.4.5",
    "@types/react": "~16.14.0",
    "@types/webpack": "~4.41.33",
    "@typescript-eslint/eslint-plugin": "~5.61.0",
    "commitizen": "~4.3.0",
    "cz-conventional-changelog": "~3.3.0",
    "esbuild": "~0.19.0",
    "eslint-config-standard": "~17.1.0",
    "eslint-config-standard-with-typescript": "~37.0.0",
    "eslint-plugin-import": "~2.27.5",
    "eslint-plugin-n": "~16.0.1",
    "eslint-plugin-promise": "~6.1.1",
    "jasmine": "~4.6.0",
    "nyc": "~15.1.0",
    "react": "~18.2.0",
    "standard-version": "~9.5.0",
    "ts-node": "~10.9.1",
    "typescript": "~5.1.6"
  },
  "peerDependencies": {
    "gatsby": ">= 2.x <= 5.x"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "files": [
    "/LICENSE",
    "/index.js",
    "/package.json",
    "/package-lock.json",
    "/gatsby-node.js",
    "/gatsby-node.js.map",
    "/gatsby-ssr.js",
    "/gatsby-ssr.js.map",
    "/interfaces.js",
    "/interfaces.js.map"
  ]
}
